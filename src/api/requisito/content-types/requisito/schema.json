{
  "kind": "collectionType",
  "collectionName": "requisitos",
  "info": {
    "singularName": "requisito",
    "pluralName": "requisitos",
    "displayName": "Requisito",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "nombre": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true,
      "maxLength": 70
    },
    "descripcion": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "text",
      "required": true
    },
    "prioridad": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "enumeration",
      "enum": [
        "Alta",
        "Media",
        "Baja"
      ],
      "required": true
    },
    "version": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "integer",
      "required": true,
      "max": 999,
      "min": 1,
      "default": 1
    },
    "esVersionActiva": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "boolean",
      "default": true,
      "required": false
    },
    "estadoRevision": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "enumeration",
      "enum": [
        "Pendiente de revisión",
        "Revisado",
        "Aprobado"
      ],
      "default": "Pendiente de revisión",
      "required": true
    },
    "ambiguedad": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::ambiguedad.ambiguedad"
    },
    "idVersionado": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::version-requisito.version-requisito",
      "inversedBy": "requisito"
    }
  }
}
