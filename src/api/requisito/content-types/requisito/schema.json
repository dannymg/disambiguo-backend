{
  "kind": "collectionType",
  "collectionName": "requisitos",
  "info": {
    "singularName": "requisito",
    "pluralName": "requisitos",
    "displayName": "Requisito",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "nombre": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true,
      "maxLength": 70
    },
    "descripcion": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "text",
      "required": true
    },
    "prioridad": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "enumeration",
      "enum": [
        "ALTA",
        "MEDIA",
        "BAJA"
      ],
      "required": true
    },
    "version": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "integer",
      "required": true,
      "max": 999,
      "min": 1,
      "default": 1
    },
    "esVersionActiva": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "boolean",
      "default": true,
      "required": false
    },
    "estadoRevision": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "enumeration",
      "enum": [
        "PENDIENTE",
        "AMBIGUO",
        "NO_AMBIGUO",
        "VALIDADO"
      ],
      "default": "PENDIENTE",
      "required": true
    },
    "ambiguedad": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::ambiguedad.ambiguedad"
    },
    "idVersionado": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::version-requisito.version-requisito",
      "inversedBy": "requisito"
    },
    "creadoPor": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "email",
      "required": true
    },
    "modificadoPor": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "email"
    }
  }
}
