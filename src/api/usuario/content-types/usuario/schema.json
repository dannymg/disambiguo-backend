{
  "kind": "collectionType",
  "collectionName": "usuarios",
  "info": {
    "singularName": "usuario",
    "pluralName": "usuarios",
    "displayName": "Usuario",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "nombreUsuario": {
      "type": "string",
      "required": true
    },
    "correo": {
      "type": "email",
      "required": true,
      "unique": true
    },
    "clave": {
      "type": "password",
      "required": true
    },
    "rol": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::rol.rol",
      "mappedBy": "usuario"
    },
    "proyectos": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::proyecto.proyecto",
      "mappedBy": "usuarios"
    }
  }
}
